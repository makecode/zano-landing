.icon
  position: relative
  display: inline-block
  background-size: cover
  background-position: center

  &.button-triangle
    width: 8px
    height: 6px
    &::after
      pseudo()
      top: 0
      left: 0
      width: 0;
      height: 0;
      border-style: solid;
      border-width: 6px 4px 0 4px;
      border-color: #ffffff transparent transparent transparent;
  
  &.download
    width: 14px
    height: 14px
    background-image: url('assets/icon-download.svg')

  &.database
